{
    "manifest_version": 2,
    "name": "天气",
    "version": "1.0",
    "description": "获取天气信息",
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "browser_action": {
        "default_action": {
            "19": "images/icon19.png",
            "38": "images/icon38.png"
        },
        "default_title": "天气",
        "default_popup": "popup.html"
    },
    "permissions": [
        "*://openweathermap.org/*"
    ],
    "options_page": "options.html"
}